Outline:
	rules*=Rule
;

Rule:
	name=ID
	'{'
		label=Label
		(icon=Icon)?
	'}'
;

Label:
	'label' '=' names+=Name['+']
;

Name:
	id=ID | string=STRING
;

Icon:
	'icon' '=' path=STRING
;

// Comments
Comment:
    CommentLine | CommentBlock
;

CommentLine:
    /\/\/.*?$/
;

CommentBlock:
    /\/\*(.|\n)*?\*\//
;
